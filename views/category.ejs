<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

	<title>Electro - HTML Ecommerce Template</title>

	<!-- Google font -->
	<link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700" rel="stylesheet">

	<!-- Bootstrap -->
	<link type="text/css" rel="stylesheet" href="stylesheets/bootstrap.min.css" />

	<!-- Slick -->
	<link type="text/css" rel="stylesheet" href="stylesheets/slick.css" />
	<link type="text/css" rel="stylesheet" href="stylesheets/slick-theme.css" />

	<!-- nouislider -->
	<link type="text/css" rel="stylesheet" href="stylesheets/nouislider.min.css" />

	<!-- Font Awesome Icon -->
	<link rel="stylesheet" href="stylesheets/font-awesome.min.css">

	<!-- Custom stlylesheet -->
	<link type="text/css" rel="stylesheet" href="stylesheets/style.css" />
	<script defer src="javascripts/jquery.min.js"></script>

	<script defer src="javascripts/jquery.nice-select.js"></script>
	<script defer src="javascripts/my-js/myjs.js"></script>


	<link rel="stylesheet" href="stylesheets/nice-select.css">
	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
		  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
		  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
		<![endif]-->

</head>

<body onload="dc.loadMoreNew(1);">
	<!-- HEADER -->
	<%include header%>
	<!-- /HEADER -->

	<!--NAV BAR-->
	<%include nav-bar%>
	<!-- BREADCRUMB -->
	<div id="breadcrumb" class="section">
		<!-- container -->
		<div class="container">
			<!-- row -->
			<div class="row">
				<div class="col-md-12">
					<ul class="breadcrumb-tree">
						<li><a href="/">Trang chủ</a></li>
						<li class="active">Tất cả sản phẩm</li>
					</ul>
				</div>
			</div>
			<!-- /row -->
		</div>
		<!-- /container -->
	</div>
	<!-- /BREADCRUMB -->

	<!-- SECTION -->
	<div class="section">
		<!-- container -->
		<div class="container">
			<!-- row -->
			<div class="row">
				<!-- ASIDE -->
				<div id="aside" class="col-md-3">
					<!-- aside Widget -->
					<div class="aside">
						<h3 class="aside-title">Danh mục</h3>
						<div class="checkbox-filter type-filter">
							<div class="input-checkbox">
								<input value="1" type="checkbox" id="category-20" onclick="dc.filter();">
								<label for="category-20">
									<span></span>
									Bất động sản
								</label>
							</div>

							<div class="input-checkbox">
								<input value="2" type="checkbox" id="category-24" onclick="dc.filter();">
								<label for="category-24">
									<span></span>
									Xe cộ
								</label>
							</div>

							<div class="input-checkbox">
								<input value="3" type="checkbox" id="category-23" onclick="dc.filter();">
								<label for="category-23">
									<span></span>
									Thời trang
								</label>
							</div>

							<div class="input-checkbox">
								<input value="4" type="checkbox" id="category-21" onclick="dc.filter();">
								<label for="category-21">
									<span></span>
									Đồ điện tử
								</label>
							</div>

							<div class="input-checkbox">
								<input value="5" type="checkbox" id="category-22" onclick="dc.filter();">
								<label for="category-22">
									<span></span>
									Giải trí
								</label>
							</div>
						</div>
					</div>
					<!-- /aside Widget -->

					<!-- aside Widget -->
					<div class="aside">
						<h3 class="aside-title">Tỉnh thành</h3>
						<div class="checkbox-filter location-filter">
							<div class="input-checkbox">
								<input value="1" type="checkbox" id="category-1" onclick="dc.filter();">
								<label for="category-1">
									<span></span>
									Hồ Chí Minh
								</label>
							</div>

							<div class="input-checkbox">
								<input value="2" type="checkbox" id="category-2" onclick="dc.filter();">
								<label for="category-2">
									<span></span>
									Hà Nội
								</label>
							</div>

							<div class="input-checkbox">
								<input value="3" type="checkbox" id="category-3" onclick="dc.filter();">
								<label for="category-3">
									<span></span>
									Đà Nẵng
								</label>
							</div>

							<div class="input-checkbox">
								<input value="4" type="checkbox" id="category-4" onclick="dc.filter();">
								<label for="category-4">
									<span></span>
									Huế
								</label>
							</div>

							<div class="input-checkbox">
								<input value="5" type="checkbox" id="category-5" onclick="dc.filter();">
								<label for="category-5">
									<span></span>
									Cần Thơ
								</label>
							</div>
						</div>
					</div>
					<!-- /aside Widget -->

					<!-- aside Widget -->
					<!-- <div class="aside">
						<h3 class="aside-title">Thời gian đăng</h3>
						<div class="checkbox-filter time-filter">
							<div class="input-checkbox">
								<input type="checkbox" id="category-6">
								<label for="category-6">
									<span></span>
									Trong ngày
									<small>(120)</small>
								</label>
							</div>

							<div class="input-checkbox">
								<input type="checkbox" id="category-7">
								<label for="category-7">
									<span></span>
									Trong tuần này
									<small>(740)</small>
								</label>
							</div>

							<div class="input-checkbox">
								<input type="checkbox" id="category-8">
								<label for="category-8">
									<span></span>
									Trong tháng này
									<small>(1450)</small>
								</label>
							</div>
						</div>
					</div> -->
					<!-- /aside Widget -->

					<!-- aside Widget -->
					<div class="aside">
						<h3 class="aside-title">Tình trang sản phẩm</h3>
						<div class="checkbox-filter status-filter">

							<div class="input-checkbox">
								<input value="1" type="checkbox" id="category-9" onclick="dc.filter();">
								<label for="category-9">
									<span></span>
									Mới hoàn toàn
									<small>(120)</small>
								</label>
							</div>

							<div class="input-checkbox">
								<input value="2" type="checkbox" id="category-10" onclick="dc.filter();">
								<label for="category-10">
									<span></span>
									Như mới
									<small>(740)</small>
								</label>
							</div>

							<div class="input-checkbox">
								<input value="3" type="checkbox" id="category-11" onclick="dc.filter();">
								<label for="category-11">
									<span></span>
									Mới vừa
									<small>(1450)</small>
								</label>
							</div>

							<div class="input-checkbox">
								<input value="4" type="checkbox" id="category-12" onclick="dc.filter();">
								<label for="category-12">
									<span></span>
									Khá mới
									<small>(1450)</small>
								</label>
							</div>

							<div class="input-checkbox">
								<input value="5" type="checkbox" id="category-13" onclick="dc.filter();">
								<label for="category-13">
									<span></span>
									Khá cũ
									<small>(1450)</small>
								</label>
							</div>
						</div>
					</div>
					<!-- /aside Widget -->


					<!-- aside Widget -->
					<div class="aside">
						<h3 class="aside-title">Giá dưới</h3>
						<div class="price-filter">
							<div id="price-slider"></div>
							<div class="input-number price-max">
								<input type="number" id="amount">
								<span class="qty-up">+</span>
								<span class="qty-down">-</span>
							</div>
							<button onclick="dc.filter();" class="button-primary">OK</button>
						</div>
					</div>
					<!-- /aside Widget -->
				</div>
				<!-- /ASIDE -->

				<!-- STORE -->
				<div id="store" class="col-md-9">
					<!-- store top filter -->
					<div class="store-filter clearfix">
						<div class="store-sort">
							<label>
								Sắp xếp
								<select id="sort-filter" class="input-select">
									<option onclick="dc.sort('normal')" value="1">Mặc định</option>
									<option onclick="dc.sort('priceAsc');" value="2">Giá thấp đến cao</option>
									<option onclick="dc.sort('priceDsc');" value="3">Giá cao đến thấp</option>
								</select>
							</label>

							<label>
								<input class="input search-txt" type="text" name="name" placeholder="Tìm kiếm sản phẩm">
							</label>
						</div>
						<ul class="store-grid">
							<li class="active"><i class="fa fa-th"></i></li>
							<li><a href="#"><i class="fa fa-th-list"></i></a></li>
						</ul>
					</div>
					<!-- /store top filter -->

					<!-- store products -->
					<div class="row" id="category-single-product-section">
						
					</div>
					<!-- /store products -->

					<!-- store bottom filter -->
					<div class="store-filter clearfix">
						<ul class="store-pagination" id="page-pagination">
							<li onclick="dc.loadMoreNew(1);" class="page-item active">1</li>
							<li onclick="dc.loadMoreNew(2);" ><a href="#">2</a></li>
							<li onclick="dc.loadMoreNew(3);" ><a href="#">3</a></li >
							<li onclick="dc.loadMoreNew(4);" ><a href="#">4</a></li>
							<li onclick="dc.loadMoreNew(5);" ><a href="#"><i class="fa fa-angle-right"></i></a></li>
						</ul>
					</div>
					<!-- /store bottom filter -->
				</div>
				<!-- /STORE -->
			</div>
			<!-- /row -->
		</div>
		<!-- /container -->
	</div>
	<!-- /SECTION -->

	<!-- NEWSLETTER -->
	<div id="newsletter" class="section">
		<!-- container -->
		<div class="container">
			<!-- row -->
			<div class="row">
				<div class="col-md-12">
					<div class="newsletter">
						<p>Sign Up for the <strong>NEWSLETTER</strong></p>
						<form>
							<input class="input" type="email" placeholder="Enter Your Email">
							<button class="newsletter-btn"><i class="fa fa-envelope"></i> Subscribe</button>
						</form>
						<ul class="newsletter-follow">
							<li>
								<a href="#"><i class="fa fa-facebook"></i></a>
							</li>
							<li>
								<a href="#"><i class="fa fa-twitter"></i></a>
							</li>
							<li>
								<a href="#"><i class="fa fa-instagram"></i></a>
							</li>
							<li>
								<a href="#"><i class="fa fa-pinterest"></i></a>
							</li>
						</ul>
					</div>
				</div>
			</div>
			<!-- /row -->
		</div>
		<!-- /container -->
	</div>
	<!-- /NEWSLETTER -->
	<%include footer%>



	<!-- jQuery Plugins -->
	<script src="javascripts/jquery.min.js"></script>
	<script src="javascripts/bootstrap.min.js"></script>
	<script src="javascripts/slick.min.js"></script>
	<script src="javascripts/nouislider.min.js"></script>
	<script src="javascripts/jquery.zoom.min.js"></script>
	<script src="javascripts/main.js"></script>
	<script src="javascripts/my-js/filter.js"></script>

</body>

</html>